{
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": false
        },
    },
    "rules": {
        "strict": 0,
        "semi": 2,
        "indent": 2,
        "camelcase": 1,
        "quotes": [1, "single"],
        "comma-dangle": [1, "always-multiline"],
        "no-unreachable": 2,
    }
}